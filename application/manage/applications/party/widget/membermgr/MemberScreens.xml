    <screen name="memberAdd">
        <section>
            <!--  <actions>
            <service service-name="findPartyConsumption"></service>
            </actions> -->
            <actions>
                <script location="component://party/webapp/membermgr/WEB-INF/actions/MemberAdd.groovy"/>
                <set field="titleProperty" value="MemberManageTitle"/>
                <!--地区选择参数-->
                <set field="dependentForm" value="MemberAddressForm"/>
                <set field="requestName" value="getAssociatedStateList"/>
                <set field="responseName" value="stateList"/>
                <set field="dependentKeyName" value="geoId"/>
                <set field="descName" value="geoName"/>
                <set field="selectedDependentOption" from-field="mechMap.postalAddress.stateProvinceGeoId" default-value="_none_"/>
                <set field="selectedDependentOption1" from-field="mechMap.postalAddress.cityGeoId" default-value="_none_"/>
                <set field="selectedDependentOption2" from-field="mechMap.postalAddress.countyGeoId" default-value="_none_"/>

                <!--根据国家选择省-->
                <set field="paramKey" value="countryGeoId"/>
                <set field="mainId" value="countryGeoId"/>
                <set field="dependentId" value="stateProvinceGeoId"/>

                <!--根据省选择市-->
                <set field="paramKey1" value="countryGeoId"/>
                <set field="mainId1" value="stateProvinceGeoId"/>
                <set field="dependentId1" value="cityGeoId"/>


                <!--根据市选择区县-->
                <set field="paramKey2" value="countryGeoId"/>
                <set field="mainId2" value="cityGeoId"/>
                <set field="dependentId2" value="countyGeoId"/>
            </actions>
            <widgets>
                <decorator-screen name="main-decorator"
                                  location="${parameters.mainDecoratorLocation}">
                    <!-- 面包屑用法 begin -->
                    <decorator-section name="pre-body">
                        <column-container>
                            <column style="body-title">
                                <label>会员管理</label>
                                <label style="sub-title">会员新增</label>
                            </column>
                            <column style="breadcrumb">
                                <label text="${uiLabelMap.CommonFirst}" style="breadcrumb"></label>
                                <link text="会员管理" style="breadcrumb"/>
                                <link text="会员新增" style="breadcrumb" target="memberAdd"/>
                            </column>
                        </column-container>
                    </decorator-section>
                    <!-- 面包屑用法 end -->
                    <decorator-section name="body">
                        <section>
                            <!-- 是否有新增权限 -->
                            <condition>
                                <if-has-permission permission="PARTYMGR_LIST" action="_CREATE"/>
                            </condition>
                            <widgets>
                                <platform-specific>
                                    <html>
                                        <html-template location="component://common/webcommon/includes/setGeoDependentDropdownValuesJs.ftl"/>
                                        <html-template location="component://party/webapp/membermgr/member/memberAdd.ftl"/>
                                    </html>
                                </platform-specific>
                            </widgets>
                            <fail-widgets>
                                <label style="h3" text="需要查看会员的新增权限"/>
                            </fail-widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
    <!--会员主数据管理 end by  gss 2016-4-5-->

