    <!--消费积分规则 begin by  gss 2016-1-16-->
    <screen name="consumeIntegralSet">
        <section>
            <actions>
                <set field="titleProperty" value="MemberManageTitle"/>
                <entity-condition entity-name="PartyLevelType" list="partyLevelList">
                </entity-condition>
                <entity-condition entity-name="PartyIntegral" list="normalList">
                    <condition-expr field-name="partyIntegralType" value="NORMAL"/>
                </entity-condition>
                <entity-condition entity-name="PartyIntegral" list="specialList">
                    <condition-expr field-name="partyIntegralType" value="SPECIALCATEGORY"/>
                </entity-condition>
                <entity-condition entity-name="PartyIntegral" list="specialProList">
                    <condition-expr field-name="partyIntegralType" value="SPECIALPRODUCT"/>
                </entity-condition>
                <entity-condition entity-name="PartyIntegral" list="nonCategoryList">
                    <condition-expr field-name="partyIntegralType" value="NONCATEGORY"/>
                </entity-condition>
                <entity-condition entity-name="PartyIntegral" list="nonProductList">
                    <condition-expr field-name="partyIntegralType" value="NONPRODUCT"/>
                </entity-condition>
                <service service-name="findPartyLevel"></service>
                <service service-name="getAllLevelType"></service>
            </actions>
            <widgets>
                <decorator-screen name="main-decorator"
                                  location="${parameters.mainDecoratorLocation}">
                    <!-- 面包屑用法 begin -->
                    <decorator-section name="pre-body">
                        <column-container>
                            <column style="body-title">
                                <label>会员管理</label>
                                <label style="sub-title">消费积分设置</label>
                            </column>
                            <column style="breadcrumb">
                                <label text="${uiLabelMap.CommonFirst}" style="breadcrumb"></label>
                                <link text="会员管理" style="breadcrumb"/>
                                <link text="消费积分设置" style="breadcrumb" target="consumeIntegralSet"/>
                            </column>
                        </column-container>
                    </decorator-section>
                    <!-- 面包屑用法 end -->
                    <decorator-section name="body">
                        <section>
                            <!-- 是否有查询权限-->
                            <condition>
                                <if-has-permission permission="PARTY_CONSUME" action="_VIEW"/>
                            </condition>
                            <widgets>
                                <platform-specific>
                                    <html>
                                        <html-template
                                                location="component://party/webapp/membermgr/member/consumeIntegralSet.ftl"/>
                                    </html>
                                </platform-specific>
                            </widgets>
                            <fail-widgets>
                                <label style="h3" text="需要查看消费积分的权限"/>
                            </fail-widgets>
                        </section>
                    </decorator-section>
                </decorator-screen>
            </widgets>
        </section>
    </screen>
